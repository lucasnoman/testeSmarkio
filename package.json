{
  "name": "testesmarkio",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lucasnoman/testeSmarkio.git"
  },
  "author": "Lucas Calhau Noman",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/lucasnoman/testeSmarkio/issues"
  },
  "homepage": "https://github.com/lucasnoman/testeSmarkio#readme",
  "husky": {
    "hooks": {
      "pre-commit": "echo 'teste'",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "3.3.0",
    "husky": "^6.0.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
